
{
  "name": "seekinsight",
  "version": "1.0.0",
  "description": "Mini Data Development Platform",
  "main": "gateway.js",
  "type": "module",
  "scripts": {
    "web": "vite",
    "webbuild": "vite build",
    "webpreview": "vite preview",
    "setup:venv": "python3 -m venv .venv || python -m venv .venv",
    "install:deps": ".venv/bin/pip install --upgrade pip && ./.venv/bin/pip install pandas sqlalchemy mysql-connector-python plotly jieba nltk wordcloud sentence_transformers || .\\.venv\\Scripts\\pip install pandas sqlalchemy mysql-connector-python plotly jieba nltk wordcloud sentence_transformers",
    "setup": "npm run setup:venv && npm run install:deps",
    "gateway": "node gateway.js",
    "start": "npm run gateway",
    "dev": "npm run setup && npm run gateway"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "mysql2": "^3.16.0",
    "prismjs": "^1.29.0",
    "@fontsource/inter": "^5.0.0",
    "@fontsource/jetbrains-mono": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "lucide-react": "^0.378.0",
    "@google/genai": "^0.1.1",
    "xlsx": "^0.18.5",
    "sql.js": "^1.10.3",
    "react-plotly.js": "^2.6.0",
    "plotly.js-dist-min": "^2.29.1"
  },
  "devDependencies": {
    "vite": "^5.2.11",
    "@vitejs/plugin-react": "^4.2.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^20.12.12"
  }
}
